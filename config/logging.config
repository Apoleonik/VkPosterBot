[loggers]
keys=root,main

[handlers]
keys=fileHandler, consoleHandler

[formatters]
keys=myFormatter

[logger_root]
level=CRITICAL
handlers=consoleHandler

[logger_main]
level=DEBUG
handlers=fileHandler
qualname=main

[handler_consoleHandler]
class=StreamHandler
level=INFO
formatter=myFormatter
args=(sys.stdout,)

[handler_fileHandler]
class=FileHandler
formatter=myFormatter
args=(sys.path[0] + "/log.txt",)

[formatter_myFormatter]
format=[%(asctime)s] | [%(levelname)s] | %(message)s
datefmt="%d-%m-%Y %H:%M:%S"